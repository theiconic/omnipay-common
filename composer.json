{
    "name": "omnipay/common",
    "description": "Common components for Omnipay payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "purchase"
    ],
    "homepage": "https://github.com/thephpleague/omnipay-common",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Macneil",
            "email": "adrian@adrianmacneil.com"
        },
        {
            "name": "Barry vd. Heuvel",
            "email": "barryvdh@gmail.com"
        },
        {
            "name": "Jason Judge",
            "email": "jason.judge@consil.co.uk"
        },
        {
            "name": "Del"
        },
        {
            "name": "Omnipay Contributors",
            "homepage": "https://github.com/thephpleague/omnipay-common/contributors"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/theiconic/omnipay-tests/"
        }
    ],
    "autoload": {
        "psr-4": { "Omnipay\\Common\\" : "src/Common" },
        "classmap": ["src/Omnipay.php"]
    },
    "require": {
        "php": ">=7.3",
        "php-http/client-implementation": "^1",
        "php-http/message": "^1.7",
        "php-http/discovery": "^1.7.4",
        "symfony/http-foundation": "^5",
        "moneyphp/money": "^3.3"
    },
    "require-dev": {
        "theiconic/omnipay-tests": "^4",
        "php-http/mock-client": "^1",
        "squizlabs/php_codesniffer": "^3.5",
        "phpro/grumphp": "^0.18"
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
